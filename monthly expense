#include <stdio.h>

int main() {
    int i, n;
    float expense, total = 0;

    printf("Enter number of expenses: ");
    scanf("%d", &n);

    for(i = 1; i <= n; i++) {
        printf("Enter expense %d: ", i);
        scanf("%f", &expense);
        total += expense;
    }

    printf("\nTotal Monthly Expense = %.2f", total);

    return 0;
}
