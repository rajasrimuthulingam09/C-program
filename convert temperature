#include <stdio.h>

float convertTemp(float c) {
    return (c * 9 / 5) + 32;
}

int main() {
    float celsius, fahrenheit;

    printf("Enter temperature in Celsius: ");
    scanf("%f", &celsius);

    fahrenheit = convertTemp(celsius);

    printf("Temperature in Fahrenheit = %.2f", fahrenheit);
    return 0;
}
